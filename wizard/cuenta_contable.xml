<odoo>
    <data>
        <record model="ir.actions.act_window" id="action_view_cuenta_contable_search">
            <field name="name">Action tqc searchclient</field>
            <field name="res_model">cuenta.contable.search</field>
            <field name="groups_id" eval="[(4, ref('base.group_system'))]"/>
        </record>

        <!--VISTA  TREE-->
        <record id="view_tree_cuenta_contable_search" model="ir.ui.view">
            <field name="name">view.tree.cuenta.contable.search</field>
            <field name="model">tqc.searchclient</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_form_cuenta_contable_search" model="ir.ui.view">
            <field name="name">view.form.cuenta.contable.search</field>
            <field name="model">cuenta.contable.search</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <form>
                    <field name="consulta" widget="cuenta_contable"/>
                    <footer class="footer-ruc">
<!--                        <button name="action_add" string="Aceptar" type="object" class="aceptar_ruc oe_highlight"/>-->
<!--                        <button string="Cancel" class="btn-secondary" special="cancel"/>-->
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>