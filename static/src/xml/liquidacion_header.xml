<?xml version="1.0" encoding="UTF-8"?>

<templates id="template">
    <t t-name="gastos_tqc.dashboard_liquid_tqc">
        <div class="o_expense_container d-flex o_form_statusbar gastos_tqc " context='{"search_default_draft": true}'>
            <div class="o_gastos_button" name="state" data-original-title="" title="">
                <button type="button" data-value="draft" id="pendiente" title="Estado actual" role="radio"
                        class="btn o_arrow_button o_context_three btn-primary"
                        context='{"search_default_contable": false,"search_default_pendiente": true}'
                        aria-current="step">
                    Pendientes a procesar
                    (<span t-esc="expenses[2]"/>)
                </button>
                <button type="button" data-value="draft" id="contable" title="Estado actual" role="radio"
                        class="btn o_arrow_button o_context_two btn-primary"
                        context='{"search_default_jefatura": false,"search_default_contable": true}'
                        aria-current="step">
                    Sin visto contable
                    (<span t-esc="expenses[1]"/>)
                </button>
                <button type="button" data-value="draft" id="jefatura" title="Estado actual" role="radio"
                        class="btn o_arrow_button o_context_one btn-primary"
                        context='{"search_default_jefatura": true,"search_default_contable": false}'
                        aria-current="step">
                    Sin visto jefe
                    (<span t-esc="expenses[0]"/>)
                </button>
            </div>

        </div>
    </t>

