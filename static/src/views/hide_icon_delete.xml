<templates xml:space="preserve">
   <t t-name="owl_learn.ClickMe.RecordRow" t-inherit="web.ListRenderer.RecordRow" owl="1">
<!--       <xpath expr="//td[hasclass('o_list_record_selector')]" position="replace">-->
<!--            <td t-on-keydown.synthetic="(ev) => this.onCellKeydown(ev, group, record)" t-if="hasSelectors"-->
<!--                class="o_list_record_selector" t-on-click.stop="() => this.toggleRecordSelection(record)" tabindex="-1">-->
<!--                <CheckBox disabled="!disabledRow(record)" value="record.selected"-->
<!--                          onChange.alike="() => this.toggleRecordSelection(record)"/>-->
<!--            </td>-->
