<odoo>
    <data>
        <record id="module_gastos_tqc" model="ir.module.category">
            <field name="name">Web de Gastos</field>
        </record>

        <record id="res_groups_administrator" model="res.groups">
            <field name="name">Gasto-Administrador</field>
            <field name="category_id" ref="gastos_tqc.module_gastos_tqc"/>
        </record>

        <record id="res_groups_employee_gastos" model="res.groups">
            <field name="name">Gasto-Empleado</field>
            <field name="category_id" ref="gastos_tqc.module_gastos_tqc"/>
        </record>

        <record id="res_groups_aprobador_gastos" model="res.groups">
            <field name="name">Gasto-Aprobador</field>
            <field name="category_id" ref="gastos_tqc.module_gastos_tqc"/>
        </record>

        <record id="res_groups_contador_gastos" model="res.groups">
            <field name="name">Gasto-Contador</field>
            <field name="category_id" ref="gastos_tqc.module_gastos_tqc"/>
        </record>


        <!-- QUITO TODOS LOS USUARIOS QUE PERTENECENA UN MODULO MENUS -->
        <record model="ir.ui.menu" id="hr.menu_config_employee">
            <field name="groups_id" eval="[(6,0,[ref('gastos_tqc.res_groups_administrator')])]"/>
        </record>
          <record model="ir.ui.menu" id="hr.menu_hr_employee">
            <field name="groups_id" eval="[(6,0,[ref('gastos_tqc.res_groups_administrator')])]"/>
        </record>
        <record model="ir.ui.menu" id="hr.menu_human_resources_configuration">
            <field name="groups_id" eval="[(6,0,[ref('gastos_tqc.res_groups_administrator')])]"/>
        </record>
        <record model="ir.ui.menu" id="hr.menu_hr_department_kanban">
            <field name="groups_id" eval="[(6,0,[ref('gastos_tqc.res_groups_administrator')])]"/>
        </record>

    </data>
</odoo>