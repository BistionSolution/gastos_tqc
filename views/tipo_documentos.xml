<odoo>
    <data>
        <record model="ir.actions.act_window" id="action_view_tipo_documentos">
        <field name="name">Action Tipo documentos</field>
        <field name="res_model">tqc.tipo.documentos</field>
        <field name="groups_id" eval="[(4, ref('base.group_system'))]"/>
        </record>

                  <!--VISTA  TREE-->
      <record id="view_tree_tipo_documentos" model="ir.ui.view">
        <field name="name">view.tree.tqc.tipo.documentos</field>
            <field name="model">tqc.tipo.documentos</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <menuitem id="menu_tipo_documentos" parent="gastos_tqc.menu_main_liquidaciones" name="Tipo documentos" action="gastos_tqc.action_view_tipo_documentos"/>
    </data>
</odoo>