<odoo>

    <record id="res_config_settings_view_form_gastos_tqc" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.gastos.tqc</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="10"/>
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <div class="app_settings_block" data-string="Gastos Tqc" string="Gastos TQC" data-key="gastos_tqc" groups="gastos_tqc.res_groups_administrator">
                    <h2>Base de datos control</h2>
                    <div class="row mt16 o_settings_container" name="catalog_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box" id="variant_options">
                            <div class="o_setting_right_pane">
                                <label for="data_base_gastos"/>
                                <field name="data_base_gastos"/>
                            </div>
                        </div>
                    </div>
                    <div class="row mt16 o_settings_container" name="catalog_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box" id="variant_options">
                            <div class="o_setting_right_pane">
                                <label for="prefix_table"/>
                                <field name="prefix_table"/>
                            </div>
                        </div>
                    </div>
                    <h2>Serie minimo y maximo caracteres</h2>
                    <div class="row mt16 o_settings_container" name="catalog_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box" id="variant_options">
                            <div class="o_setting_right_pane">
                                <label for="max_serie"/>
                                <field name="max_serie"/>
                            </div>
                        </div>
                    </div>
                    <div class="row mt16 o_settings_container" name="catalog_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box" id="variant_options">
                            <div class="o_setting_right_pane">
                                <label for="min_serie"/>
                                <field name="min_serie"/>
                            </div>
                        </div>
                    </div>
                    <h2>Establecer contraseña</h2>
                    <div class="row mt16 o_settings_container" name="catalog_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box" id="variant_options-2">
                            <div class="o_setting_right_pane">
                                <label for="pass_user_gastos"/>
                                <field name="pass_user_gastos"/>
                            </div>
                            <button string="Cambiar contraseña" name="action_calcular" type="object" class="oe_highlight"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

